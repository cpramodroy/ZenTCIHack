     <extension
         point="com.tibco.bw.design.ActivitySignature">
      <signature
            activityTypeID="bw.zendesk.CreateTicket"
            signatureClass="com.tibco.bw.palette.zendesk.design.createticket.CreateTicketSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.CreateUser"
            signatureClass="com.tibco.bw.palette.zendesk.design.createuser.CreateUserSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.UpdateTicket"
            signatureClass="com.tibco.bw.palette.zendesk.design.updateticket.UpdateTicketSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.Delete"
            signatureClass="com.tibco.bw.palette.zendesk.design.delete.DeleteSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.GetTicketMetrics"
            signatureClass="com.tibco.bw.palette.zendesk.design.getticketmetrics.GetTicketMetricsSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.Search"
            signatureClass="com.tibco.bw.palette.zendesk.design.search.SearchSignature">
      </signature>
      <signature
            activityTypeID="bw.zendesk.GetTickets"
