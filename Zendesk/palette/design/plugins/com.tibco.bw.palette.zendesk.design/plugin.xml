            activityTypeID="bw.zendesk.CreateTicket"
            name="%zendesk.activity.CreateTicket.name"
            type="Standard"
            priority="100"
            tooltip="%zendesk.activity.CreateTicket.tooltip"
            visible="true">
         <palette
               paletteID="bw.zendesk">
         </palette>
      </activityType>

      <activityType
