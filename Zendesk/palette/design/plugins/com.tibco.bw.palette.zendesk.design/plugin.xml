<plugin>

   <extension
         point="com.tibco.bw.design.Palette">
      <paletteDrawer
            expanded="false"
            largeIcon="icons/obj32/zendesk_32x32.png"
            name="%zendesk.palette.name"
            paletteID="bw.zendesk"
            priority="500"
            productID="bw.zendesk.product"
            smallIcon="icons/obj16/zendesk_16x16.png"
            tooltip="%zendesk.palette.tooltip">
      </paletteDrawer>
   </extension>
   
